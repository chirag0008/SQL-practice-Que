create table emp(empno int,sal int,ename varchar(20));
alter table emp add rownum int not null auto_increment primary key;
insert into emp(empno,sal,ename)values(7839,5000,"ilomy");
insert into emp(empno,sal,ename) values(3232,10000,"eancd");
insert into emp(empno,sal,ename) values(5678,6000,"sdfm");
insert into emp(empno,sal,ename ) values(3212,4567,"mllr");
-- display names with length 4
select ename from emp where ename like "____";
-- display  the names of emp  whose name contains the (i)second letter as 'L' (ii)fourth letter as 'M'
select ename from emp where ename like "%ll%";
